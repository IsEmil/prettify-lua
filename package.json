{
	"name": "prettify-lua",
	"displayName": "prettify lua",
	"description": "what",
	"version": "0.1.2",
	"publisher": "IsEmil",
	"icon": "",
	"repository": "https://github.com/IsEmil/prettify-lua",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "Prettify Lua",
			"properties": {
				"prettify-lua.LuaminOutput": {
					"type": "string",
					"default": "Replace current file",
					"description": "Where to output luamin'd script",
					"enum": [
						"Replace current file",
						"Create new file",
						"Copy to clipboard"
					],
					"enumDescriptions": [
						"Replaces entire document with luamin'd script",
						"Creates new document with luamin'd script",
						"Copies luamin'd script to the clipboard"
					]
				}
			}
		},
		"commands": [
			{
				"command": "prettify-lua.luamin",
				"title": "Herrtt's Luamin to Minify and Beautify"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.0"
	}
}
